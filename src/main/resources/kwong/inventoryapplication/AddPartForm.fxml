<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddPartController">
   <children>
      <Label layoutX="34.0" layoutY="48.0" text="Add Part">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <RadioButton fx:id="addPartInHouseRBtn" layoutX="129.0" layoutY="39.0" mnemonicParsing="false" onAction="#selectRBtnInHouse" prefHeight="44.0" prefWidth="89.0" selected="true" text="In-House">
         <toggleGroup>
            <ToggleGroup fx:id="partRBtnToggleGroup" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="addPartOutsourceRBtn" layoutX="302.0" layoutY="39.0" mnemonicParsing="false" onAction="#selectRBtnOutsource" prefHeight="44.0" prefWidth="89.0" text="Outsourced" toggleGroup="$partRBtnToggleGroup" />
      <Label layoutX="32.0" layoutY="103.0" prefHeight="44.0" prefWidth="80.0" text="ID" />
      <Label layoutX="32.0" layoutY="147.0" prefHeight="44.0" prefWidth="71.0" text="Name" />
      <Label layoutX="32.0" layoutY="191.0" prefHeight="44.0" prefWidth="71.0" text="Inv" />
      <Label layoutX="30.0" layoutY="235.0" prefHeight="44.0" prefWidth="71.0" text="Price/Cost" />
      <Label layoutX="32.0" layoutY="278.0" prefHeight="44.0" prefWidth="71.0" text="Max" />
      <Label layoutX="302.0" layoutY="282.0" prefHeight="37.0" prefWidth="38.0" text="Min" />
      <Label fx:id="addPartMachineIdLabel" layoutX="32.0" layoutY="329.0" prefHeight="44.0" prefWidth="96.0" text="Machine ID" />
      <TextField fx:id="addPartMinTxt" layoutX="347.0" layoutY="288.0" />
      <TextField fx:id="addPartMachineIdTxt" layoutX="129.0" layoutY="339.0" />
      <TextField fx:id="addPartMaxTxt" layoutX="129.0" layoutY="288.0" />
      <TextField fx:id="addPartPriceTxt" layoutX="129.0" layoutY="245.0" />
      <TextField fx:id="addPartInvTxt" layoutX="129.0" layoutY="200.0" />
      <TextField fx:id="addPartNameTxt" layoutX="129.0" layoutY="156.0" />
      <TextField fx:id="addPartIdTxt" layoutX="129.0" layoutY="113.0" />
      <Button fx:id="addPartSaveBtn" layoutX="278.0" layoutY="406.0" mnemonicParsing="false" onAction="#addPartOnActionSavePart" prefHeight="25.0" prefWidth="52.0" text="Save" />
      <Button fx:id="addPartCancelBtn" layoutX="347.0" layoutY="406.0" mnemonicParsing="false" onAction="#addPartOnActionCancelPart" prefHeight="25.0" prefWidth="61.0" text="Cancel" />
   </children>
</AnchorPane>
